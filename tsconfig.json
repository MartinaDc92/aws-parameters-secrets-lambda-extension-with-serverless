{
    "compileOnSave": false,
    "compilerOptions": {
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
          "esnext"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "./dist/out-tsc",
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "target": "es2020",
      "typeRoots": [
          "node_modules/@types"
      ]
    }
  }